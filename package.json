{
  "name": "aurt-frontend-hooks",
  "version": "1.1.0",
  "description": "AURT Restaurant",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 3001",
    "now-build": "next build",
    "start": "next start",
    "export": "npm run now-build && next export",
    "test": "NODE_ENV=test jest --watchAll",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "heroku-postbuild": "next build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.0",
    "compression": "^1.7.3",
    "date-fns": "^2.0.0-alpha.7",
    "downshift": "^2.2.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "markdown-to-jsx": "^6.9.0",
    "next": "9.4.4",
    "next-offline": "^5.0.2",
    "next-router-events": "^2.1.0",
    "next-with-apollo": "^3.1.3",
    "nextjs-sitemap-generator": "^0.0.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "16.13.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "16.13.1",
    "react-ga": "^2.5.7",
    "react-iframe": "1.3.3",
    "react-instafeed": "^1.0.0",
    "react-intl": "^2.8.0",
    "react-pdf": "^4.0.5",
    "react-pdf-js": "^4.0.2",
    "react-responsive": "^6.1.1",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.5.0",
    "reactjs-popup": "^1.3.2",
    "rodal": "^1.6.3",
    "styled-components": "^5.1.1",
    "use-abortable-fetch": "^0.3.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "enzyme-to-json": "^3.3.4",
    "graphql-tools": "^4.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "raw-loader": "^1.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
